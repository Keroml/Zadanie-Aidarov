\b integer\b0   K, Q, R, q [0: 10], r [0: 10];\
\b long integer stack\b0   C, U, V, W;\
\b control stack\b0   code;\
\b integer\b0   k, p, s, t, i, j;\
\b long integer\b0   X, Y, Z, w;\
\b control\b0   A;\
Шаг 1;\
Шаг 2;\
\b while\b0   code не пуст \b do\b0  \
\b while\b0   k > 1 \b do\b0  \
шаг 5; шаг 6; шаг 7; шаг 8;\
\b end\b0  ;\
шаг 9;\
\b pop\b0   code \b into\b0   A; \b while\b0   А = интерполировать \b do\b0  \
шаг 11;\
\b for\b0   i=1 \b to\b0   2t \b do\b0   шаг 13;\
\b for\b0   i=2t \uc1\u8722? 1 \b downto\b0   1 \b do\b0   шаг 15;\
шаг 16;\
\b end\b0  ;\
